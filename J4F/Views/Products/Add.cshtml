
@{
    Layout = null;
}
<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>三水数据报送系统</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link href="~/css/iview.css" rel="stylesheet" />
    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/vue.min.js"></script>
    <script src="~/js/iview.min.js"></script>
    <script src="~/js/HS_Util.js"></script>
    <script src="~/js/app.js"></script>
    <link href="~/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/css/app.css" rel="stylesheet" />
    <style type="text/css">
        .contain {
            width: 90%;
            height: 100%;
            margin-top: 0px;
            margin-left: 5%;
        }

        .contain_title1 {
            font-size: 14px;
            display: block;
            width: 80px;
            height: 10px;
            position: relative;
            top: 37px;
            left: 30px;
            text-align: center;
            background: white;
        }

        .contain_button {
            width: 75%;
            margin-left: 25%;
            text-align: right;
        }

        .ivu-btn {
            margin-left: 10px;
            padding: 3px 15px;
        }

        .ivu-icon {
            cursor: pointer;
        }

        #app p {
            text-align: center;
            font-family: 宋体;
            font-size: 18px;
            font-weight: bold;
        }

        #form1 table {
            width: 100%;
            height: 100%;
            border: 1px solid #BEBEBE;
            margin-top: 8px;
        }

            #form1 table td {
                font-size: 14px;
                padding: 4px 0px 4px 0px;
            }

        .ivu-input {
            font-size: 14px;
        }

        [v-cloak] {
            display: none
        }

        .ivu-upload {
            width: 90px;
            margin-left: -5px;
        }
    </style>
</head>
<body>
    <div id="app"></div>
    <script type="text/x-template" id="list">
        <div class="contain">
            <span class="contain_title1">任务定义</span>
            <div class="contain_button">
                <i-button :loading="saving" @@click="savePage" v-if="pageobj.zt!=='使用中'">
                    <span id="save1" v-if="!saving">保存</span>
                    <span v-else>保存中</span>
                </i-button>
                <i-button onclick="CloseLayer()">关闭</i-button>
            </div>
            <i-form id="form1">
                <table cellpadding="0" cellspacing="10" style=" padding: 13px;">
                    <tr>
                        <td style=" width: 11%" align="right">数据名称：</td>
                        <td style=" width: 32%; padding: 0px;"><i-input v-model="pageobj.sjmc" placeholder="请输入..." style="width: 100%;"></i-input></td>
                        <td style=" width: 11%" align="right">版本号：</td>
                        <td style=" width: 32%; padding: 0px;"><i-input v-model="pageobj.bbh" placeholder="请输入..." style="width: 100%;"></i-input></td>
                    </tr>
                    <tr>
                        <td align="right">数据类型：</td>
                        <td style=" padding: 0px;">
                            <i-select v-model="pageobj.sjlx" placeholder="">
                                <i-option value="文档">文档</i-option>
                                <i-option value="格式化报表">格式化报表</i-option>
                                <i-option value="在线填报">在线填报</i-option>
                            </i-select>
                        </td>
                        <td align="right">状态：</td>
                        <td style=" padding: 0px;">
                            <i-select v-model="pageobj.zt" placeholder="">
                                <i-option value="草稿">草稿</i-option>
                                <i-option value="使用中">使用中</i-option>
                                <i-option value="已停止">已停止</i-option>
                            </i-select>
                        </td>
                    </tr>
                    <tr>
                        <td align="right">备注：</td>
                        <td colspan="3" style=" padding: 0px;">
                            <i-input type="textarea" :autosize="{minRows: 1,maxRows: 3}" v-model="pageobj.bz" placeholder="请输入..." style="width: 100%;"></i-input>
                        </td>
                    </tr>
                </table>
            </i-form>
        </div>
    </script>
    <script>var thispagedata = {};</script>
    <script src="~/js/products/add.js"></script>
</body>
</html>

